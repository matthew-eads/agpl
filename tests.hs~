module Agpl_tests where
import Test.HUnit
import Parser
import Agpl_syntax

ttt = "TTT: Game\n\nGamestate:{Board: {[[Int]]}\nPiece:{Char}\nTurn:{Int}\n}\n"
      ++ "Player:{Char}\nMove:{(Int, Int)}\nisValid:{undefined}\npossMoves:"
      ++ "{undefined}\noutcome:{undefined}\ninitialState:{undefined}\n"

tttTest = TestCase (assertEqual "Tic Tac Toe" (Game ([Board (AppT ListT (AppT ListT (ConT Int))),Piece (ConT Char),Turn (ConT Int)],Move (AppT (AppT (
TupleT 2) (ConT Int)) (ConT Int)),IsValidFun (VarE undefined),PossMovesFun (VarE undefined),OutcomeFun 
(VarE undefined),InitState (VarE undefined),Player (ConT Char),[])) ttt
